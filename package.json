  "scripts": {
    "start": "webpack --config webpack.server.config.js & npm run backend-node & npm run dev-server",
    "dev-server": "npm run sass-dev & webpack-dev-server --host 0.0.0.0 --content-base-target 'http://127.0.0.1:5000' --colors",
